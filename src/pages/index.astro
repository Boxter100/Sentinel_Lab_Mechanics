---
import RenderFull from '../components/renderFull.astro';
import Layout from '../layouts/Layout.astro';
import Prueba from '../components/prueba.astro';
import Tokyo from '../components/tokyo.astro';
---

<Layout>
	<Prueba />
	<RenderFull max={600} preload={20} height="300dvh"/>
	<Prueba />
	<RenderFull id="2" path="/frames" max={807} preload={20} height="300dvh" startFrame={600}/>
	<Prueba />
	<Tokyo   modelPath="/models/tu-modelo.glb"
  backgroundColor="#eeeeee"
  modelScale={0.5}
  cameraPosition={3} />
</Layout>
